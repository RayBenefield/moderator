{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "indent": [
            "error",
            4,
            { "SwitchCase": 1 }
        ],
        "no-plusplus": 0,
        "no-mixed-operators": 0,
        "no-param-reassign": 0,
        "no-continue": 0
    }
}
